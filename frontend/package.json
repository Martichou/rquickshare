    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-vue": "9.19.2",
    "postcss": "8.4.32",
    "postcss-import": "15.1.0",
    "postcss-nesting": "12.0.1",
    "tailwindcss": "3.4.0",
